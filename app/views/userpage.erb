<div>
  <nav>
     <a href="/" class="btn-floating pulse" > home</a>
     <a href="/user/edit"> Edit profile</a>
  </nav>
</div>


<h2>Posts</h2>

<%@user.car_posts.each do |post|%>
  <div class="row">
    <div class="col s12 m6 ">
      <div class="card blue-grey darken-3">
        <div class="card-content white-text">
          <span class="card-title"><%=post.model%></span>
             <p>Make:<%= post.make%></p>
             <p>Model:<%= post.model%></p>
             <p>Miles: <%=post.miles%></p>
             <p>Year: <%=post.year%></p>
             <p>Price: $<%=post.price%></p>
             </div>
          <div class="card-action">
             <form action="/posts/<%= post.id %>/edit" method="get">
               <button class="waves-effect waves-light btn-small">Edit</button>
             </form>
             <form action="/posts/<%= post.id %>" method="post">
               <input type="hidden" name="_method" value="delete">
               <input class="waves-effect waves-light btn-small right red" type="submit" value="Delete">
             </form>
          </div>
       </div>
     </div>
   </div>
<%end%>           
             
<%@user.create_houseposts.each do |housepost| %>
  <div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-3">
        <div class="card-content white-text">
          <span class="card-title"><%=housepost.address%></span>
             <p> bd:<%=housepost.badroom%></p>
             <p> ba:<%=housepost.bathroom%> </p> 
             <p> sqf:<%=housepost.sqf%></p>  
             <p>address:<%=housepost.address%>, city:<%=housepost.city%>, zip code:<%=housepost.zip_code%>, state:<%=housepost.country%> </p>
             <h5>Price: $<%=housepost.price%> </h5>
             </div>
           <div class="card-action">
          <form action="/house/<%= housepost.id%>/edit" method="get">
           <button class="waves-effect waves-light btn-small">Edit</button>
          </form>   
          <form action="/house/<%= housepost.id %>" method="post">
            <input type="hidden" name="_method" value="delete">
            <input class="waves-effect waves-light btn-small right red" type="submit" value="Delete">
        </div>
      </div>
    </div>
  </div>
<%end%>       
  
  
           





 






          


 





   
