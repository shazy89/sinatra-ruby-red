<div>
  <a href="/" class="btn-floating pulse ">home</a>
</div>
<div class="center">    
  <div class="col s12 m2">
    <h1 class="z-depth-4" >Welcome to Ruby Red </h1>
  </div>
</div>
<%house_posts.each do |post|%>
  <div class="row ">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <h5>$<%=post.price%></h5>
          <p> br:<%=post.badroom%> ba:<%=post.bathroom%> sqf:<%=post.sqf%></p>
          <p><%=post.address%> <%=post.city%> <%=post.badroom%> <%=post.state%> <%=post.country%> </p>
        </div>
        <div >
        <details>
          <summary>Details</summary>
            <p>Seller: <%=post.user.firstname.capitalize%></p>
            <p>contact: <%=post.user.contact%></p>
        </div>
      </div>
    </div>
  </div>
<%end%>          
   


