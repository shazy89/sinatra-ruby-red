<div class="col s12 m2">
    <h1 class="z-depth-4" >Welcome to Ruby Red </h1>
    </div>
   
<%@all.each do |post|%>
  <div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">Card Title</span>
         <h4>Seller: <%=post.user.firstname%></h4>
<h5 class="z-depth-2" > 
   Make: <%=post.make%>, 
   Model: <%=post.model%>, 
   Miles: <%=post.miles%>, 
   Year: <%=post.year%>, 
   Price: $<%=post.price%>
</h5>
        </div>
        <div class="card-action">
          <a href="#">This is a link</a>
          <a href="#">This is a link</a>
        </div>
      </div>
    </div>
  </div>
<%end%>

     
