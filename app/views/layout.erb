  <body class="#e8eaf6 indigo lighten-5">
    <div class="container">
      <nav>
        <ul>
            <% if logged_in? %>
            <li class="right"><a class="waves-effect waves-light btn-small" href="/logout" >Logout</a></li>
            <li class="left"><a class="waves-effect waves-light btn-small" href="/user/create">create posts</a></li>
             <li class="left" ><a class="waves-effect waves-light btn-small" href="/userpage">User's page</a></li>
       
            <%else %>
            <li class="right"><a href="/signup">Create Account</a></li>
            <li class="right"><a href="/login">Login</a></li>
            <%end%>
           </ul>
      </nav>   
 
       <% if flash[:error] %> 
       <% flash[:error].each do |error| %>
           <div>
              <%= error %>
          </div>
             <% end %>
        <% end %>
         <% if flash[:notice] %>
          <div>
            <%= flash[:notice] %>
          </div>
        <% end %>

<div class="container grey lighten-4 z-depth-5" style="margin-top: 25px; margin-bottom: 25px; padding: 25px;">
        <%= yield %>

     
    </div>
      <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
   
