<div>
  <a href="/" class="btn-floating pulse ">home</a>
</div>
<div class="center">    
  <div class="col s12 m2">
    <h1 class="z-depth-4" >Welcome to Ruby Red </h1>
  </div>
</div>

<div class="row">
<%@all.each do |post|%>

    <div class="col s12 m6 l4">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">For Sale <%=post.make%> </span>
            <li> Make: <%=post.make%>, </li>
            <li> Model: <%=post.model%>, </li>
            <li> Miles: <%=post.miles%>, </li>
            <li>  Year: <%=post.year%>, </li>
            <li> Price: $<%=post.price%> </li>
        </div>
        <div >
        <details>
          <summary>Details</summary>
            <p>Seller: <%=post.user.firstname%></p>
            <p>contact: <%=post.user.contact%></p>
      </div>
    </div>
  </details>
</div>
<%end%>
</div>  

         



